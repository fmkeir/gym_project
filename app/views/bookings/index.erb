<h1>Bookings</h1>

<table>
  <tr>
    <th>Member</th>
    <th>Session</th>
    <th>Trainer</th>
  </tr>
  <% for booking in @bookings %>
  <tr>
    <td><%= booking.member().full_name() %></td>
    <td><%= booking.session().type() %></td>
    <td><%= booking.session().trainer() %></td>
    <td><a href="/members/<%= booking.member.id %>"> View member</a></td>
    <td><a href="/sessions/<%= booking.session.id %>"> View session</a></td>
  </tr>
  <% end %>
</table> <br/>

<form class="" action="/bookings/new" method="get">
  <input type="submit" value="Create new booking">
</form>
